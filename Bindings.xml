    <Binding name="HEKILI_SNAPSHOT" header="HEKILI_HEADER">Hekili:MakeSnapshot();</Binding>
    <Binding name="HEKILI_TOGGLE_COOLDOWNS" header="HEKILI_HEADER">Hekili:ToggleCooldowns();</Binding>
